@{
}
<!DOCTYPE html>
<html lang="en">
<head></head>
<body>
    <fieldset class="scheduler-border">
        <legend class="scheduler-border" id="legend">List Des Parcours</legend>
        <table class="table">
            <thead>
                <tr>
                    <th>
                        Code
                    </th>
                    <th>
                        CodeFiliere
                    </th>
                    <th>
                        IntituleAr
                    </th>
                    <th>
                        IntituleAbrege
                    </th>
                    <th>
                        IntituleFr
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in @ViewBag.ListParcours)
                {
                    <tr>
                        <td>
                            @item.Code
                        </td>
                        <td>
                            @item.CodeFiliere
                        </td>
                        <td>
                            @item.IntituleAr
                        </td>
                        <td>
                            @item.IntituleAbrege

                        </td>
                        <td>
                            @item.IntituleFr
                        </td>
                    </tr>

                }
            </tbody>
        </table>
    </fieldset>
</body>
</html>